{
  "name": "Notebag",
  "version": "1.0",
  "description": "Turn your computer into one intelligent and faithful assistant",
  "browser_action": {
    "default_icon": "icon.png",
    "name":"Open note"
  },
  "content_scripts":[{
      "matches":["https://*/*","http://*/*","*://*/*"],
      "js":["src/note_page.js"]
  }],
  "permissions": [
        "tabs","http://*/*","https://*/*","*://*/*"
  ],
  "background_page":"src/background.html",
  "manifest_version":2
}